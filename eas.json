{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "BASE_URL": "$BASE_URL",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "$EXPO_PUBLIC_GEOAPIFY_API_KEY",
        "API_KEY": "$API_KEY",
        "apiKey": "$apiKey",
        "authDomain": "$authDomain",
        "databaseURL": "$databaseURL",
        "projectId": "$projectId",
        "storageBucket": "$storageBucket",
        "messagingSenderId": "$messagingSenderId",
        "appId": "$appId",
        "measurementId": "$measurementId"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "BASE_URL": "$BASE_URL",
        "EXPO_PUBLIC_BASE_URL": "$EXPO_PUBLIC_BASE_URL",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "$EXPO_PUBLIC_GEOAPIFY_API_KEY",
        "API_KEY": "$API_KEY",
        "EXPO_PUBLIC_API_KEY": "$EXPO_PUBLIC_API_KEY",
        "apiKey": "$apiKey",
        "authDomain": "$authDomain",
        "databaseURL": "$databaseURL",
        "projectId": "$projectId",
        "storageBucket": "$storageBucket",
        "messagingSenderId": "$messagingSenderId",
        "appId": "$appId",
        "measurementId": "$measurementId"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
